{
  "name": "startwars",
  "version": "0.0.1",
  "description": "star wars API to mongodb collection",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --no-cache --passWithNoTests --runInBand --detectOpenHandles --verbose --coverage --forceExit",
    "start": "node src/index.js",
    "dev": "nodemon ./src/index.js  --watch .env --watch ./src",
    "compose:up": "docker compose up --build",
    "compose:down": "docker compose down -v"
  },
  "keywords": [
    "starwars",
    "swapi"
  ],
  "engines": {
    "node": "20.x"
  },
  "author": "Jone Polvora",
  "license": "MIT",
  "dependencies": {
    "amqplib": "^0.10.3",
    "axios": "^1.6.2",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "envalid": "^8.0.0",
    "express": "^4.18.2",
    "http-graceful-shutdown": "^3.1.13",
    "mongodb": "^6.3.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  }
}
